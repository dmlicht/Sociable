<% unless current_user?(@user) %>
  <div id="wing_form">
    <% if current_user.wing?(@user) %>
      <%= render 'remove_wing' %>
    <% elsif current_user.pending_wing?(@user) %>
      <p> wing pending </p>
    <% elsif pair_request = current_user.requested_wing?(@user) %>
      <%= render :partial => 'shared/accept_pair', :locals => {:pair_request => pair_request} %>
      <%= render :partial => 'shared/reject_pair', :locals => {:pair_request => pair_request} %>
    <% else %>
      <%= render 'add_wing' %>
    <% end %>
  </div>
<% end %>
